{
  "manifest_version": 3,
  "name": "foldable",
  "description": "Allows you to easily fold and unfold Swagger UI tabs",
  "version": "0.1",
  "action": {
    "default_popup": "info.html",
    "default_icon": "images/icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["src/styles/output.css", "images/assets/arrow-down.svg" ],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://localhost:*/*",
        "https://*.r-tech.addcode.ru/*"
      ],
      "js": ["scripts/index.js"],
      "css": ["src/styles/output.css"]
    }
  ]
}